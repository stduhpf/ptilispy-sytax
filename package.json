{
    "name": "ptilispy-sytax",
    "displayName": "ptilispy_sytax",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.55.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "lsp",
            "aliases": ["ptiLis.py", "lsp"],
            "extensions": [".lsp"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "lsp",
            "scopeName": "source.lsp",
            "path": "./syntaxes/lsp.tmLanguage.json"
        }]
    },
    "configurationDefaults": {
        "[languageId]": {
            "editor.semanticHighlighting.enabled": true
        }
    }
}